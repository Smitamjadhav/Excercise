<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
  
<suite name="GoogleSearchSuite">
  <test name="ChromeGoogleSearch" >
  <parameter name="browser" value="chrome" />
    <classes>
       <class name="com.test.SearchWithEnteredTerms" />
    </classes>
  </test>
  <test name="EdgeGoogleSearch" >
  <parameter name="browser" value="edge" />
    <classes>
       <class name="com.test.SearchWithEnteredTerms" />
    </classes>
  </test>
  <test name="FireFoxGoogleSearch" >
  <parameter name="browser" value="firefox" />
    <classes>
       <class name="com.test.SearchWithEnteredTerms" />
    </classes>
  </test>	
    <test name="SearchWithOutEnteredTerms" >
      <classes>
       <class name="com.test.SearchWithOutEnteredTerms" />
    </classes>
  </test>
 </suite>











































